{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint:latest",
    "tslint-config-prettier"
  ],
  "jsRules": {
    "no-unused-expression": true
  },
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "member-access": [
      false
    ],
    "ordered-imports": [
      false
    ],
    "max-line-length": false,
    "member-ordering": [
      false
    ],
    "interface-name": [
      false
    ],
    "arrow-parens": false,
    "object-literal-sort-keys": false,
    "max-classes-per-file": false,
    "comment-format": false,
    "no-this-assignment": false,
    "no-console": false
  },
  "rulesDirectory": []
}
